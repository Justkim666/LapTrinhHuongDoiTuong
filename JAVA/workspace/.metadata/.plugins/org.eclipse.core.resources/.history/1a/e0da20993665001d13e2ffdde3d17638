package ThuVien;

import java.util.Scanner;

public class TapChi extends TaiLieu{
	private int soPhatHanh;
	private int thangPhatHanh;
	
	public TapChi() {
		super();
		soPhatHanh = thangPhatHanh = 1;
	}
	
	public TapChi(TapChi tc) {
		soPhatHanh = tc.soPhatHanh;
		thangPhatHanh = tc.thangPhatHanh;
	}
	
	public void nhap() {
		Scanner sc = new Scanner(System.in);
		super.nhap();
		System.out.println("Nhap vao so phat hanh: "); soPhatHanh = sc.nextInt();
		System.out.println("Nhap vao thang phat hanh: "); thangPhatHanh = sc.nextInt();
	}
}

import java.util.Scanner;

public class KhuPho {
	private HoGiaDinh khuPho[];
	
	public KhuPho() {
		khuPho = new HoGiaDinh[100];
	}
	
	public KhuPho(KhuPho kp) {
		khuPho = kp.khuPho;
	}
	
	public void nhap() {
		Scanner sc = new Scanner(System.in);
		int n;
		System.out.print("Nhap so ho gia dinh: "); n = sc.nextInt();
	}
}

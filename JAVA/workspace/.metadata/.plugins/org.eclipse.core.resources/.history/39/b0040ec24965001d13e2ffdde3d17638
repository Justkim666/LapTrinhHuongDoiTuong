
public class Nguoi {
	private String hoTen, ngheNghiep, CMND;
	private int tuoi;
	
	public Nguoi() {
		hoTen = new String();
		ngheNghiep = new String();
		CMND = new String();
		tuoi = 1;
	}
	
	public Nguoi(Nguoi n) {
		hoTen = new String(n.hoTen);
		ngheNghiep = new String(n.ngheNghiep);
		CMND = new String(n.CMND);
		tuoi = n.tuoi;
	}
}

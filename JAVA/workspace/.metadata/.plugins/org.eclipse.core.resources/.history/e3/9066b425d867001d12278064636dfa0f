
public class Gach {
	private String maSo, mau;
	private int sLuong, cdai, cngang;
	private float gia;
	
	public Gach() {
		maSo = new String();
		mau = new String();
		sLuong = cdai = cngang = 0;
		gia = 0.0f;
	}
	
	public Gach(Gach g) {
		maSo = new String(g.maSo);
		mau = new String(g.mau);
		sLuong = g.sLuong;
		cdai = g.cdai;
		cngang = g.cngang;
		gia = g.gia;
	}
}
